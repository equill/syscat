      attributes:
        description:
          description: Comments about this particular AZ.
  awsAccounts:
      attributes:
        name:
        description:
          description: Comments about this particular account.
  awsVpcs:
      dependent: true
      attributes:
          dhcpoptionsid:
          instancetenancy:
          isdefault:
          state:
